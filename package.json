    "axios": "^0.24.0",
    "chai": "^4.2.0",
    "chromedriver": "^107.0.3",
    "csv-load-sync": "^1.0.0",
    "cucumber-html-reporter": "^5.5.0",
    "cucumber-pretty": "^6.0.1",
    "date-fns": "^2.22.1",
    "diff": "^5.1.0",
    "dotenv": "^16.0.3",
    "downloads-folder": "^3.0.1",
    "html-dnd": "^1.2.1",
    "jira-client": "^8.2.2",
    "jira-client-xray": "^1.0.1",
    "jshint": "^2.13.6",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "objects-to-csv-file": "^1.0.1",
    "papaparse": "^5.3.2",
    "pdf-parse": "^1.1.1",
