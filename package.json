    "selenium-webdriver": "^4.0.0-alpha.4",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0",
    "xlsx": "^0.18.5",
    "xray-jira-reporter": "^2.1.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/chai": "^4.1.7",
    "@types/cucumber": "^7.0.0",
    "@types/node": "^18.11.9",
    "@types/selenium-webdriver": "^4.0.1",
    "cucumber": "^6.0.7",
    "cucumber-parallel": "^2.0.3",
    "pactum": "^3.2.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
