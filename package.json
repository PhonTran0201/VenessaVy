{
  "name": "pegasus-core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "henry": "cucumber-js -p default features/insurance/feature_daily_smoke_test/1_Account_ConvertLead_SearchSale.feature",
    "hogse": "cucumber-js -p default features/guarantee/HogSE/smoke-test/1.ByggsakerhetCombinedBond.feature",
    "daily1": "cucumber-js -p default features/insurance/feature_daily_smoke_test/1_*",
    "daily2": "cucumber-js -p default features/insurance/feature_daily_smoke_test/2_*",
    "daily3": "cucumber-js -p default features/insurance/feature_daily_smoke_test/3_*",
    "leone-test1": "cucumber-js -p default Leone1",
    "leone-uat": "cucumber-js -p default Leone_UAT",
    "test2": "cucumber-js -p default Backup",
    "tenant-setting": "npx ts-node ./src/shared/tenant-setting/tenant-setting-get-object.ts",
    "ir": "cucumber-js -p default features/insurance/feature_regression/5-*",
    "gs": "cucumber-js -p default features/guarantee/*",
    "gi": "cucumber-js -p default features/guarantee/instalment/1*",
    "gh": "cucumber-js -p default features/guarantee/Hogm/4*",
    "ci0": "cucumber-js --parallel 2 -p default feature_ci",
    "ci": "cucumber-js -p default feature_ci",
    "ci1": "cucumber-js -p default feature_ci",
    "hogs-smoketest": "cucumber-js -p default features/guarantee/Hogm/Smoke_test_manual/*",
    "report": "node reporter.js",
    "debuger": "cucumber-js -p default debuger/*",
    "varsam": "cucumber-js -p default features/feature_regression_varsam/*",
    "varsam-staging": "cucumber-js -p default features/insurance/varsam/feature_varsam_staging_tenant/*",
    "varsam-preview": "cucumber-js -p default features/insurance/varsam/feature_varsam_preview_tenant/*",
    "data-generation": "ts-node src/core/modals/data_generate.ts",
    "varsam-smoketest-staging": "cucumber-js --tags \"@SmokeTest\" -p default features/insurance/varsam/feature_varsam_staging_tenant/*.feature",
    "varsam-smoketest-staging_cp": "cucumber-js --tags \"@SmokeTest\" -p default features/insurance/varsam/feature_varsam_staging_tenant/BOP_CP/*.feature",
    "varsam-smoketest-preview": "cucumber-js --tags \"@SmokeTest\" -p default features/insurance/varsam/feature_varsam_preview_tenant_smoke_test/*.feature",
    "varsam-smoketest-uat": "cucumber-js --tags \"@SmokeTest and @UAT and @Varsam and not @Skip\" -p default features/insurance/varsam/feature_varsam_preview_tenant_smoke_test/*.feature",
    "varsam-migration": "cucumber-js --tags \"@Staging and @Customer\" -p default features/migration/*/*",
    "run-parallel": "node --max-old-space-size=4096 varsam-migration-parallel.js",
    "varsam-migration-parallel": "cucumber-js --parallel 8 --format progress --fail-fast --tags \"@Staging and @Checklist\" -p default features/parallel/*/*",
    "dryrun-parallel": "cucumber-js --parallel 4 -dry-run --fail-fast --tags \"@Staging and @Team\" -p default features/parallel/*/*",
    "varsam-migration-prod": "cucumber-js --tags \"@Staging and @Customer\" -p default features/migration/*/*",
    "varsam-jenkins": "cucumber-js --tags \"@Staging and @Varsam and @SmokeTest\" -p default features/migration/varsam/*.feature",
    "varsam-migration-uat": "cucumber-js --tags \"@UAT\" -p default features/migration/varsam/*.feature",
    "varsam-selftest": "cucumber-js --tags \"@selftest\" -p default features/migration/varsam/*.feature",
    "varsam-apitest": "cucumber-js --tags \"@Staging and @APITest\" -p default features/migration/varsam/*.feature",
    "rerun": "cucumber-js @rerun.txt",
    "testregression": "cucumber-js ${npm_config_tags} --publish-quiet -p default features/migration/varsam/*.feature",
    "testexecution": "ts-node src\\core\\XRayHandler.ts"
  },
  "nodeOptions": [
    "--max-old-space-size=8192"
  ],
  "keywords": [],
  "author": "Jolie",
  "license": "ISC",
